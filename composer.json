{
    "name": "authress/authress-sdk.php",
    "type": "libray",
    "description": "Client SDK for Authress authorization as a service. Provides managed authorization api to secure service resources including user data.",
    "keywords": [
        "authorization",
        "authorization as a service",
        "authentication",
        "user authentication",
        "Authress",
        "Authress client",
        "access management",
        "access management as a service",
        "user security"
    ],
    "homepage": "https://authress.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Authress Developers",
            "homepage": "https://authress.io/app/#/support"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "*",
        "friendsofphp/php-cs-fixer": "*",
        "symfony/phpunit-bridge": "^5.2"
    },
    "autoload": {
        "psr-4": { "AuthressSdk\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "AuthressSdk\\" : "test/" }
    }
}
