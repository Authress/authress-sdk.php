{
    "name": "authress/authress-sdk.php",
    "type": "libray",
    "description": "Client SDK for Authress authorization as a service. Provides managed authorization api to secure service resources including user data.",
    "keywords": [
        "authorization",
        "authorization as a service",
        "authentication",
        "user authentication",
        "Authress",
        "Authress client",
        "access management",
        "access management as a service",
        "user security"
    ],
    "homepage": "https://authress.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Authress Developers",
            "homepage": "https://authress.io/app/#/support"
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "lcobucci/jwt": "^4.1",
        "guzzlehttp/guzzle": "~6.5.6 || ~7.4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "*",
        "friendsofphp/php-cs-fixer": "*",
        "symfony/phpunit-bridge": "^5.2"
    },
    "autoload": {
        "psr-4": { "AuthressSdk\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": {
            "AuthressSdk\\" : "lib/"
        }
    }
}
